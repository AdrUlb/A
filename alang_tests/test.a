func printInt(value: u16)
{
	print(value);
}

func fib(a: u16): u16
{
	if (a == 0)
		return 0;

	if (a == 1)
		return 1;

	return fib(a - 1) + fib(a - 2);
}

func main()
{
	printInt(fib(4));
}
